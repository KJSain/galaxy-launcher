max_upload_size: "50g"

galaxy_web_urls:
  - example_host.org
  - galaxy.example_host.org
  - www.example_galaxy.org

# remote listening ports
public_galaxy_web_port: 80

# Local listening ports
galaxy_web_port: 8080
#galaxy_ftp_port: 8021
#galaxy_sftp_port: 8022
# By default ftp and sftp ports are only exposed to localhost.
# Set below values to True to expose them to the internet.
galaxy_docker_expose_ftp_port: False
galaxy_docker_expose_sftp_port: False
